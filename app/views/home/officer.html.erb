<h1>Welcome Officer</h1>

<br>

<% if officer_signed_in? %>
    <%= link_to 'Sign Out', destroy_officer_session_path, method: :delete, class:"btn btn-secondary" %><br>
    <%= link_to 'Edit', edit_officer_registration_path, class:"btn btn-secondary" %><br>
<% else %>
    <%= link_to 'Sign Up', new_officer_registration_path, class:"btn btn-secondary" %><br>
    <%= link_to 'Sign In', new_officer_session_path, class:"btn btn-secondary" %><br>
<% end %>